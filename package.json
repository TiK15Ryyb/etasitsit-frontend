{
  "name": "latvala-fireworks",
  "version": "0.2.1",
  "description": "A portal for new year's fireworks offers from Säästötalo Latvala in Lohja",
  "main": "main.js",
  "repository": "git@bitbucket.org:OyAimoLatvalaAb/latvala-fireworks.git",
  "author": "Oy Aimo Latvala Ab / Axel Latvala",
  "license": "MIT",
  "private": true,
  "dependencies": {},
  "scripts": {
    "start": "webpack-dev-server --hot --inline --config webpack.dev.js --progress",
    "package": "webpack --config webpack.prod.js",
    "clean": "rm -rf ./builds ./dist",
    "lint": "eslint --ext \".js,.jsx\" src/",
    "esfix": "eslint --fix --ext \".js,.jsx\" src/",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24",
    "babel-loader": "^8.0.4",
    "core-js": "^3",
    "css-loader": "^1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-prop-type-error": "^1.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^3.0.0",
    "webpack": "^4",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "webpack-shell-plugin-next": "^1.1.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFiles": [
      "./src/setupTests.js",
      "jest-prop-type-error"
    ],
    "browser": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/src/node_modules**"
    ],
    "verbose": false
  }
}
